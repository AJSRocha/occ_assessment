  title = {Biogeographical, Biochemical and Genetic Differentiation among {{North American}} Saccoglossids ({{Hemichordata}}; {{Enteropneusta}}; {{Harrimaniidae}})},
  author = {King, G. M. and Giray, C. and Kornfield, I.},
  year = {1995},
  month = aug,
  journal = {Marine Biology},
