@article{erzini_comparative_2002,
  title = {A Comparative Study of the Species Composition of Discards from {\textregistered}ve {\textregistered}sheries from the {{Algarve}} (Southern {{Portugal}})},
  author = {Erzini, K and Costa, M E and Bentes, L and Borges, T C},
  year = {2002},
  journal = {Fisheries Management and Ecology},
  pages = {10},
  abstract = {Discards from {\textregistered}ve of the most important {\textregistered}sheries (crustacean trawling, {\textregistered}sh trawling, demersal purse seining, pelagic purse seining and trammel netting) in southern Portugal were studied and compared. A total of 236 species of all taxa were discarded, with {\textregistered}sh and cephalopods accounting for more than 90\% of the discarded biomass, except trammel nets (81\%). Although there was some overlapping of species, multivariate analysis using cluster analysis for classi{\textregistered}cation and multidimensional scaling (MDS) for ordination, showed that there were signi{\textregistered}cant dierences between the {\textregistered}ve gears in terms of species composition and biomasses discarded, with the least similarity between crustacean trawling and all other {\textregistered}shing operations. The dierences between {\textregistered}sheries were probably as a result of a combination of gear selectivity and depth {\textregistered}shed. The results suggest that discarding practices are likely to have dierent, yet signi{\textregistered}cant impacts on the marine ecosystem, warranting further studies on the fate of discards, the factors in{\textasciimacron}uencing discarding and mitigation.},
  langid = {english},
  file = {C:\Zotero\storage\MZUNQA96\Erzini et al. - 2002 - A comparative study of the species composition of .pdf}
}

