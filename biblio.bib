  title = {Reconstructing Individual Shape Histories of Fish Otoliths: {{A}} New Image-Based Tool for Otolith Growth Analysis and Modeling},
  shorttitle = {Reconstructing Individual Shape Histories of Fish Otoliths},
  author = {Fablet, Ronan and Chessel, Anatole and Carbini, Sebastien and Benzinou, Abdesslam and {de Pontual}, H{\'e}l{\`e}ne},
  year = {2009},
  month = mar,
