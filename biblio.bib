@article{masuda_jack_2008,
  title = {Jack Mackerel {{Trachurus}} Japonicus Juveniles Use Jellyfish for Predator Avoidance and as a Prey Collector},
  author = {Masuda, Reiji and Yamashita, Yoh and Matsuyama, Michiya},
  year = {2008},
