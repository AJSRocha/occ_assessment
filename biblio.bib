@article{isaza-toro_standardization_2020,
  title = {Standardization of Small Purse Seiner Fishing Effort and Its Relation to Fishing Grounds in the Eastern Tropical Sector of the {{Eastern Pacific Ocean}}},
  author = {{Isaza-Toro}, Estefania and Giraldo, Alan and Selvaraj, John Josephraj and {Ort{\'i}z-Ferr{\'i}n}, Omar Olimpo},
  year = {2020},
  month = sep,
  journal = {Regional Studies in Marine Science},
  volume = {39},
  pages = {101432},
  issn = {23524855},
  doi = {10.1016/j.rsma.2020.101432},
  urldate = {2024-05-27},
  abstract = {Considering that fishing grounds could be locations where repetitive fishing efforts are carried out to guarantee economic gain, we analyzed the spatial distribution of the standardized effort associated with the foreign purse seine tuna fleet that operated in eastern tropical sector of the Eastern Pacific Ocean between 2009 and 2015, monitored by the Colombian Fisheries Observer Program. The standardization of the fishing power and fishing effort of 17 tuna vessels began with a principal component analysis applied to a correlation matrix calculated for purse seine length, height, and area, carrying capacity, vessel length, tonnage, and beam. The first principal component explained 72.58\% of the variance and was used to develop a multiple linear regression model from which the mathematical expression to standardize fishing power (FP) was derived: FPvessel = 1 + (1.84 {$\cdot$} 12.10-1) {$\cdot$} C1. Fishing effort was standardized by multiplying the fishing power by the nominal effort of each vessel. Maps of the spatial distribution of these efforts as indicators of fishing grounds were then created. The highest concentration of fishing effort occurred between 2{\textopenbullet} and 4{\textopenbullet} N, and from the coastline to 82{\textopenbullet} W, coinciding with fishing grounds known as ``Banco Tumaco'', ``Pasacaballos'', and ``Banco Colombia'' in the southeastern part of the colombian Exclusive Economic Zone.},
  langid = {english},
  file = {C:\Zotero\storage\K3CQ9DLE\Isaza-Toro et al. - 2020 - Standardization of small purse seiner fishing effo.pdf}
}

