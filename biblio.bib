@article{machias_bottom_2001,
  title = {Bottom Trawl Discards in the Northeastern {{Mediterranean Sea}}},
  author = {Machias, A and Vassilopoulou, V and Vatsos, D and Bekas, P and Kallianiotis, A and Papaconstantinou, C and Tsimenides, N},
  year = {2001},
  month = oct,
  journal = {Fisheries Research},
  volume = {53},
  number = {2},
  pages = {181--195},
  issn = {01657836},
  doi = {10.1016/S0165-7836(00)00298-8},
  urldate = {2020-08-25},
  abstract = {Discard practices of trawlers in three main areas of the Aegean and western Ionian Seas were examined. Data collected on board commercial vessels during 3 years (1995{\textpm}1998) of seasonal (Autumn, Winter, Spring) monitoring were used to estimate discarded quantities. About 44\% (range 39{\textpm}49\%) of the total catch was discarded at sea (13 500{\textpm}22 000 t annually); the main component of the discards was {\textregistered}sh. Hauls were classi{\textregistered}ed into two main clusters using the discard quantities of {\textregistered}sh, crustaceans, and cephalopods. The two clusters were discriminated by depth. Two sets of equations that classify new hauls in the clusters, using depths and duration of the hauls or marketable yield, were applied. The discarded yield of {\textregistered}sh showed more precise relationships with their marketable yield in each season than did crustaceans and cephalopods. The latter were strongly affected by the speci{\textregistered}c characteristic of the different areas and clusters. \# 2001 Elsevier Science B.V. All rights reserved.},
  langid = {english},
  file = {C\:\\Zotero\\storage\\F8Z5CCXC\\Machias et al. - 2001 - Bottom trawl discards in the northeastern Mediterr.pdf;C\:\\Zotero\\storage\\IMQUCPEQ\\Machias et al. - 2001 - Bottom trawl discards in the northeastern Mediterr.pdf;C\:\\Zotero\\storage\\TBRIULRI\\Machias et al. - 2001 - Bottom trawl discards in the northeastern Mediterr.pdf}
}

