@article{booth_life_1997-1,
  title = {On the Life History of the Lesser Gurnard ({{Scorpaeniformes}}: {{Triglidae}}) Inhabiting the {{Agulhas Bank}}, {{South Africa}}},
  shorttitle = {On the Life History of the Lesser Gurnard ({{Scorpaeniformes}}},
  author = {Booth, A. J.},
