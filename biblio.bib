  shorttitle = {Embryonic and Larval Development of the Tub Gurnard {{{\emph{Trigla}}}}{\emph{ Lucerna}} ({{Pisces}}},
  author = {Dul{\v c}i{\'c}, J. and Grubi{\v s}i{\'c}, L. and Katavi{\'c}, I. and Skakelja, N.},
  year = {2001},
  month = apr,
  journal = {Journal of the Marine Biological Association of the United Kingdom},
  volume = {81},
  number = {2},
  pages = {313--316},
  issn = {0025-3154, 1469-7769},
  doi = {10.1017/S0025315401003794},
  urldate = {2020-08-03},
  abstract = {The embryonic and early larval development of the laboratory-reared tub gurnard,               Trigla lucerna               are described. The eggs ranged in diameter from 1{$\cdot$}33 to 1{$\cdot$}40\hspace{0.166em}mm, with a mean of 1{$\cdot$}36{\textpm}0{$\cdot$}236\hspace{0.166em}mm, were pelagic and spherical with a homogeneous and unsegmented yolk. There was single oil globule in the eggs. Globule ranged in diameter from 0{$\cdot$}25 to 0{$\cdot$}29\hspace{0.166em}mm (mean 0{$\cdot$}28{\textpm}0{$\cdot$}012\hspace{0.166em}mm). Embryonic development lasted 115\hspace{0.166em}h 2\hspace{0.166em}min at mean temperature 13{$\cdot$}5\hspace{0.166em}{$^\circ$}C. Newly-hatched larvae were 3{$\cdot$}09{\textpm}0{$\cdot$}014 mm in total length. Absorption of the yolk sac was complete after the sixth day, when larvae reached 4{$\cdot$}85{\textpm}0{$\cdot$}015\hspace{0.166em}mm in total length. The only character for identifying eggs among species could be diameter, since the values obtained are lower than those obtained for               Eutrigla gurnardus               and               Aspitrigla cuculus               , but larger than those for               Trigloporus lastoviza               . The length of newly hatched yolk-sac larvae of tub gurnard is significantly lower (               t               -test,               P               {$<$}0{$\cdot$}05) than those of most of the other gurnard species. Characters likely to help in identification of species will probably prove to be the length, shape and pigmentation of the pectoral fin, and the degrees of development of spiny armature on the head.},
  langid = {english},
  file = {C:\Zotero\storage\DCT9VJ6M\Dulčić et al. - 2001 - Embryonic and larval development of the tub gurnar.pdf}
}

