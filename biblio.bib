@article{borges_sardine_2003,
  title = {Sardine Regime Shifts off {{Portugal}}: A Time Series Analysis of Catches and Wind Conditions},
  shorttitle = {Sardine Regime Shifts off {{Portugal}}},
  author = {Borges, M. F. and Santos, A. M. P. and Crato, N. and Mendes, H. and Mota, B.},
  year = {2003},
  month = apr,
  journal = {Scientia Marina},
  volume = {67},
  number = {S1},
  pages = {235--244},
  issn = {1886-8134, 0214-8358},
  doi = {10.3989/scimar.2003.67s1235},
  urldate = {2024-02-09},
  abstract = {Decadal changes have been observed in the annual catch of sardine (Sardina pilchardus W). Long-term changes have also been observed in alongshore winds off Portugal in the last decades in winter months. During sardine spawning season (winter), northerly winds that favour upwelling led to unfavourable conditions for egg and larval survival. By using time series analysis, we investigated the effect of wind conditions and North Atlantic Oscillation (NAO) on the sardine catches, in the period from 1946-1991. We also investigated the time lag between recruitment strength and its turnout in catches. We concluded that recruitment is forced to a lower level when the frequency and intensity of northerly wind exceeds a certain limit in winter. Our time series retrospective analyses led to evidence of climatic driven regime-shifts in West Portugal sardine productivity in late 1960s-early 1970s. These results are discussed in terms of their implications in fisheries management plans.},
  langid = {english},
  file = {C:\Zotero\storage\CWK55B9X\Borges et al. - 2003 - Sardine regime shifts off Portugal a time series .pdf}
}

