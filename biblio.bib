@article{bartoli_revision_2007,
  title = {A Revision of {{Lomasoma Manter}} 1935 ({{Digenea}}: {{Fellodistomidae}}), with New Data on the Type-Specimens of Four Species and the Description of {{L}}. Triglae n. Sp. from {{Chelidonichthys}} Lucernus ({{L}}.) ({{Pisces}}: {{Triglidae}}) in the {{Western Mediterranean}}},
