  author = {Szalaj, D. and Torres, M.A. and {Veiga-Malta}, T. and Ang{\'e}lico, M.M. and {Sobrinho-Gon{\c c}alves}, L. and Chaves, C. and Alcoforado, B. and Garrido, S. and R{\'e}, P. and Cabral, H. and Silva, A.},
