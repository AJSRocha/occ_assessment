@article{vazquez-rowe_estimating_2011,
  title = {Estimating Global Discards and Their Potential Reduction for the {{Galician}} Fishing Fleet ({{NW Spain}})},
  author = {{V{\'a}zquez-Rowe}, Ian and Moreira, Ma Teresa and Feijoo, Gumersindo},
  year = {2011},
