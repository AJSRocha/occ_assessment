@article{borges_by-catch_nodate,
  title = {By-Catch and Discarding Practices in {\textregistered}ve {{Algarve}} (Southern {{Portugal}}) {{me{\^A}}} Tiers},
  author = {Borges, T C and Erzini, K and Bentes, L and Costa, M E},
  pages = {12},
  abstract = {The catches and discards of trawlers, seiners and trammel netters were studied in the Algarve (southern Portugal) using observers onboard commercial {\textregistered}shing vessels. Species diversity was high, with 236 species recorded as occasionally, frequently or regularly discarded. Mean discard rates per trip were 0.13, 0.20, 0.27, 0.62 and 0.70, respectively, for trammel nets, demersal purse seines, pelagic purse seines, {\textregistered}sh trawls and crustacean trawls, with high variability in terms of discard volume and discard rate. This was especially so for seiners where from 0 to 100\% of the total catch might be discarded. No signi{\textregistered}cant relationships were found between discard quantity or discard rate and characteristics of the {\textregistered}shing vessels sampled within each me{\^A} tier (total length, TAB, hp, kW). Based on the above discard rates and the ocial landings, it was estimated that in 1996, Algarve trawlers discarded 9000{\textpm}13000 tonnes (t) while seiners discarded 5500{\textpm}8200 t. Discarding practices in these {\textregistered}sheries are reviewed and the reasons for discarding are presented by species and me{\^A} tier basis. The results support the need for more studies on the factors contributing to discarding, variability in discard quantities and rates, the fate of discards and their importance to the marine ecosystem, and on ways of reducing by-catch and discarding in these {\textregistered}sheries.},
  langid = {english},
  file = {C:\Zotero\storage\W5U6QBL5\Borges et al. - By-catch and discarding practices in ®ve Algarve (.pdf}
}

