@incollection{jones_distribution_2000,
  title = {Distribution and Abundance of Co-Occurring Chthamalid Barnacles {{Chthamalus}} Montagui and {{Chthamalus}} Stellatus ({{Crustacea}}, {{Cirripedia}}) on the Southwest Coast of {{Portugal}}},
  booktitle = {Island, {{Ocean}} and {{Deep-Sea Biology}}},
  author = {Sousa, E. B. and Cruz, T. and Castro, J. J.},
