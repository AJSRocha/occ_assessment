@article{santos_weightlength_2002,
  title = {{{Weight}}{\textpm}length Relationships for 50 Selected {\textregistered}sh Species of the {{Algarve}} Coast (Southern {{Portugal}})},
  author = {Santos, M N and Gaspar, M B and Vasconcelos, P and Monteiro, C C},
  year = {2002},
  journal = {Fisheries Research},
  pages = {7},
  abstract = {This study reports weight{\textpm}length relationships for 50 selected {\textregistered}sh species of the Algarve coast (southern Portugal). Specimens were caught during {\textregistered}shing surveys with different types of {\textregistered}shing gears (trawls, nets, long-lines and traps) or sampled during whole {\textregistered}sh market sampling operations. A total of 22 825 individuals were sampled, belonging to 50 {\textregistered}sh species from 25 families. The best represented was family Sparidae (13 species), distantly followed by Scombridae and Triglidae (four species), Carangidae (three species), and by Gadidae, Haemulidae, Scyliorhinidae, Serranidae and Soleidae (two species). In numerical terms, the most important species were Trachinus draco and Diplodus bellottii, with 2767 and 2144 individuals, respectively, followed by Citharus linguatula (1844), D. vulgaris (1206), Microchirus azevia (1165), Scorpaena notata (1118) and Pagellus erythrynus (1075). To the author's best knowledge, this study presents the {\textregistered}rst references on weight{\textpm}length relationships for six {\textregistered}sh species worldwide, for 15 species for the northeastern Atlantic, for 20 species for Portuguese waters and for 25 species for the Algarve coast (southern Portugal).},
  langid = {english},
  file = {C\:\\Zotero\\storage\\AQ79WZA3\\Santos et al. - 2002 - Weight±length relationships for 50 selected ®sh sp.pdf;C\:\\Zotero\\storage\\P4PC32DA\\Santos et al. - 2002 - Weight±length relationships for 50 selected ®sh sp.pdf;C\:\\Zotero\\storage\\SVJW9SXJ\\Santos et al. - 2002 - Weight±length relationships for 50 selected ®sh sp.pdf;C\:\\Zotero\\storage\\WL2CFLJQ\\Santos et al. - 2002 - Weight±length relationships for 50 selected ®sh sp.pdf}
}

