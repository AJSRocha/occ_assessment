@article{noauthor_universidade_nodate-1,
  title = {{UNIVERSIDADE DO ALGARVE}},
  abstract = {A total of 1008 pairs of otoliths of Trachurus picturatus (Bowdich, 1825), were observed from individuals landed by the commercial fishing fleet between July 2003 and June 2004. Age readings were made on ethanol immersed whole otoliths. Different validation procedures were used to validate the age readings, namely back-calculation, monthly analysis of otoliths edge type and marginal increment. Age readings consistency and precision were assessed by measuring the error of different age readings performed by the author and between readers. The individuals sampled were divided in six age groups from 0 to V, without significant differences between males and females (P{$>$}0,05). The equation that better described the relationship between the length of fish and the otoliths radius was the equation of Dahl-Lea (LTi=(Ri/Rc) LTc ). The back-calculated mean lengths showed the inversed of Lee's phenomenon, and they were not significantly different (P{$>$}0,05) from the mean lengths at age estimated directly from the age-length key. Age reading's quality were assessed by using the Average Percent Error and Chang's Variation Index. The Percentage of Aggreement and Calibration Model were used to assess age readings between readers. The results obtained with the different methods were coherent and consistent. The values estimated for the monthly marginal increment and the analysis of the age type evolution confirmed the formation of one annual growth zone. The growth parameters of von Bertalanffy for T. picturatus were L{$\infty$}=42,6 cm, k=0,142 year-1 and t0=--2,673 year. The growth in weight is isometric with W{$\infty$}=727,6 g.},
  langid = {portuguese},
  file = {C:\Zotero\storage\VYJ79WFE\UNIVERSIDADE DO ALGARVE.pdf}
}

