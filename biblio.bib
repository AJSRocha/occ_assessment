  author = {Petitgas, P and Alheit, J and Peck, Ma and Raab, K and Irigoien, X and Huret, M and Van Der Kooij, J and Pohlmann, T and Wagner, C and Zarraonaindia, I and {Dickey-Collas}, M},
  year = {2012},
