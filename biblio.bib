@article{prista_reproductive_2014,
  title = {Reproductive Phase Determination in Male Meagre ({{Argyrosomus}} Regius, {{Sciaenidae}}): Testis Development and Histologic Corroboration of a Gross Anatomical Scale},
  shorttitle = {Reproductive Phase Determination in Male Meagre ({{Argyrosomus}} Regius, {{Sciaenidae}})},
  author = {Prista, Nuno and Gordo, Leonel and Lino Costa, Jos{\'e} and Costa, Maria Jos{\'e} and Jones, Cynthia},
