@article{fernandes_discards_2010,
  title = {Discards {{Raising Procedures}} for {{Portuguese}} Trawl Fleet -- {{Revision}} of Methodologies Applied in Previous Years},
  author = {Fernandes, Ana Cl{\'a}udia and Jardim, Ernesto and Pestana, Gra{\c c}a},
  year = {2010},
  abstract = {Discards estimates presented in previous years were obtained using total number of trips of each fleet as raising factor. Once there is heterogeneity between trips duration within a stratum, and few numbers of sampled trips in North area of Portugal, this working document pretends to analyse in what way they interfere with discards estimates. A generalized linear model (GLM) and graphical analysis were performed to evaluate how discards are related to factors and variables such as year, fleet, fishing area, fishing depth, fishing time, and number of days of trip. Neither fishing area nor fishing depth interferes with discards in weight. However a relation between discards and number of days of trip was observed. Consequently also fishing time is related with discards. Total discards estimation was calculated using fishing time as auxiliary variable. The use of fishing time in this procedure resulted in lower discards estimates and CV's when compared to the one obtained using trips as auxiliary variable. The same method was used to get length composition of hake discards. Big differences in length composition were also obtained when comparing both methods.},
