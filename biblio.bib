  shorttitle = {Increasing Biological Knowledge for Better Management of By-Catch Species},
  author = {Sequeira, Vera and Sousa, In{\^e}s and Neves, Ana and Rita Vieira, Ana and Silva, Elisabete and Silva, Frederica and Duarte, Ana Marta and Mendes, Susana and Ganh{\~a}o, Rui and Assis, Carlos Alberto and Rebelo, Rui and Magalh{\~a}es, Maria Filomena and Gil, Maria Manuel and Serrano Gordo, Leonel},
  year = {2023},
