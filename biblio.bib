@article{pardo_phylogenetic_2005,
  title = {Phylogenetic Analysis of Flatfish ({{Order Pleuronectiformes}}) Based on Mitochondrial 16s {{rDNA}} Sequences},
  author = {Pardo, Bel{\'e}n G. and Machordom, Annie and Foresti, Fausto and {Porto-Foresti}, F{\'a}bio and Azevedo, Marisa F. C. and Ba{\~n}on, Rafael and S{\'a}nchez, Laura and Mart{\'i}nez, Paulino},
