@article{cabral_discards_2003,
  title = {Discards of the Beach Seine Fishery in the Central Coast of {{Portugal}}},
  author = {Cabral, Henrique and Duque, Joel and Costa, Maria Jos{\'e}},
  year = {2003},
  month = jul,
  journal = {Fisheries Research},
