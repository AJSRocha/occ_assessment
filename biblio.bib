@article{cardinale_reconstructing_2009,
  title = {Reconstructing Historical Trends of Small Pelagic Fish in the {{Java Sea}} Using Standardized Commercial Trip Based Catch per Unit of Effort},
  author = {Cardinale, Massimiliano and Nugroho, Duto and Hernroth, Lars},
  year = {2009},
