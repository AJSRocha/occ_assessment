@article{voulgaridou_trends_2003,
  title = {Trends in Various Biological Parameters of the {{European}} Sardine, {{{\emph{Sardina}}}}{\emph{ Pilchardus}} ({{Walbaum}}, 1792), in the {{Eastern Mediterranean Sea}}},
  author = {Voulgaridou, Pinelopi and Stergiou, Konstantinos I.},
  year = {2003},
