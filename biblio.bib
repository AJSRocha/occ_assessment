@article{perales-raya_age_2010,
  title = {Age Estimation Obtained from Analysis of Octopus ({{Octopus}} Vulgaris {{Cuvier}}, 1797) Beaks: {{Improvements}} and Comparisons},
  shorttitle = {Age Estimation Obtained from Analysis of Octopus ({{Octopus}} Vulgaris {{Cuvier}}, 1797) Beaks},
  author = {{Perales-Raya}, Catalina and Bartolom{\'e}, Aurora and {Garc{\'i}a-Santamar{\'i}a}, M. Teresa and {Pascual-Alay{\'o}n}, Pedro and Almansa, Eduardo},
