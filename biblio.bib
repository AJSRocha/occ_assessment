@article{giannoulaki_habitat_2011,
  title = {Habitat Suitability Modelling for Sardine Juveniles ({{Sardina}} Pilchardus) in the {{Mediterranean Sea}}: {{Habitat}} Suitability Modelling for Sardine Juveniles},
