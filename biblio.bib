@article{queiroga_oceanographic_2007,
  title = {Oceanographic and Behavioural Processes Affecting Invertebrate Larval Dispersal and Supply in the Western {{Iberia}} Upwelling Ecosystem},
  author = {Queiroga, Henrique and Cruz, Teresa and Dos Santos, Antonina and Dubert, Jesus and {Gonz{\'a}lez-Gordillo}, Juan Ign{\'a}cio and Paula, Jos{\'e} and Peliz, {\'A}lvaro and Santos, A. Miguel P.},
