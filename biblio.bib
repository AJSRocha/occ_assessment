@article{veiga-malta_first_2019,
  title = {First Representation of the Trophic Structure and Functioning of the {{Portuguese}} Continental Shelf Ecosystem: Insights into the Role of Sardine},
  shorttitle = {First Representation of the Trophic Structure and Functioning of the {{Portuguese}} Continental Shelf Ecosystem},
  author = {{Veiga-Malta}, T and Szalaj, D and Ang{\'e}lico, Mm and Azevedo, M and Farias, I and Garrido, S and Louren{\c c}o, S and Mar{\c c}alo, A and Marques, V and Moreno, A and Oliveira, Pb and Paiva, Vh and Prista, N and Silva, C and {Sobrinho-Gon{\c c}alves}, L and Vingada, J and Silva, A},
