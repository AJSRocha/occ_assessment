@article{vasconcelos_bycatch_2019,
  title = {Bycatch, Discards and Survival Rates in the Small-scale Cuttlefish Trap Fishery on Sandy Bottoms off the {{Algarve}} Coast (Southern {{Portugal}})},
  author = {Vasconcelos, Paulo and Pereira, F{\'a}bio and Moreno, Ana and Gaspar, Miguel B.},
  year = {2019},
