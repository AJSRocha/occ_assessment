@article{costa_growth_2017,
  title = {Growth and Distribution of Mackerel {{Scomber}} Scombrus {{Linnaeus}}, 1758 from the West Coast of {{Portugal}}},
  author = {COSTA, Ana Maria and GORDO, Leonel and MARTINS, Maria Manuel},
  year = {2017},
  publisher = {Station Biologique de Roscoff (SBR)},
  issn = {2262-3094},
  doi = {10.21411/CBM.A.FCACD9B0},
  urldate = {2024-02-09},
  abstract = {This paper studies the growth of 7379 Atlantic mackerels (Scomber scombrus Linnaeus, 1758), 3626 males, 3726 females and 27 immature, caught by the commercial fleet, during 2009-2013 off the Portuguese coast (ICES Division IXa South). Age determination was done by counting the annual growth rings on otoliths from 3301 individuals. The studied fish showed a range of lengths from 16 to 49 cm (mean length of 29.7 cm) with ages between 0 and 15 years old (mean age of 2.8 years). The mean lengths at age were also found, corresponding to 20.5 cm for age 0 and 26.0 cm for age 1. No differences in the von Bertalanffy growth parameters between sexes were found but statistical differences were recorded between years. The length-weight relationships pointed to a positive allometric growth (b {$>$} 3.0) and statistical differences were found between sexes and among the five years. The information achieved in this study was compared with the data for mackerel from other geographic areas of the NE Atlantic. The results provide additional information on the growth of this species in the less studied part of the southern stock component - the Portuguese continental waters.},
  langid = {english},
  file = {C:\Zotero\storage\SVBZBKPH\COSTA et al. - 2017 - Growth and distribution of mackerel Scomber scombr.pdf}
}

